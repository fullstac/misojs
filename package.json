{
  "name": "misojs",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "rundev": "nodemon --watch v/ --watch cfg/ --watch c/ --watch m/ --watch client/ --watch server/ --watch app.js app.js",
    "start": "node app.js"
  },
  "bin": {
    "miso": "bin/nodemvc.bin.js"
  },
  "dependencies": {
    "async": "*",
    "express": "~4.10.6",
    "mithril": "*",
    "mithril-node-render": "*",
    "grunt": "*",
    "lodash": ">=2.4.1",
    "nodemon": "^1.2.1",
    "npm": "~2.1.16"
  },
  "description": "MISOJS (Mithril ISOmorphic JavaScript) is a node.js and mithril based MVC implementation that provides an isomorphic environment to create apps quickly.",
  "main": "app.js",
  "directories": {
    "test": "test"
  },
  "devDependencies": {
    "mocha": "~2.0.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jsguy/misojs.git"
  },
  "keywords": [
    "mvc",
    "mithril",
    "isomorphic",
    "framework",
    "useful",
    "jsguy"
  ],
  "author": "Mikkel Bergmann",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jsguy/misojs/issues"
  }
}
